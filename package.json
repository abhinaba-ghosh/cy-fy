{
  "name": "cypress-automation-docker",
  "version": "0.0.0",
  "description": "This package is to install cypress and reporters",
  "main": "index.js",
  "scripts": {
    "slack": "cypress-slack-reporter --logger",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:gherkin": "gherkin-lint -c ./.gherkin-lintrc cypress/**/*.feature",
    "lint": "npm run lint:fix && npm run lint:gherkin",
    "cy:cucumber:open": "cypress open --browser chrome",
    "cy:cucumber:run": "cypress run --browser chrome",
    "allure:report": "allure generate reports/allure-results --clean -o reports/allure-report",
    "allure:clear": "del-cli reports/**",
    "allure:history": "mv -f reports/allure-report/history reports/allure-results/history && rm -r reports/allure-report || true"
  },
  "dependencies": {
    "@slack/client": "^5.0.1",
    "cucumber": "6.0.5",
    "cypress": "^4.5.0",
    "cypress-cucumber-preprocessor": "2.0.1",
    "cypress-failed-log": "^2.4.0",
    "sonarqube-scanner": "^2.1.2",
    "tsify": "^4.0.1"
  },
  "devDependencies": {
    "@applitools/eyes.cypress": "3.4.11",
    "@shelex/cypress-allure-plugin": "^1.6.0",
    "@testing-library/cypress": "4.2.0",
    "@types/cypress-cucumber-preprocessor": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "chai": "4.2.0",
    "coffeescript": "^2.5.1",
    "cypress-slack-reporter": "0.5.5",
    "del-cli": "^3.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-unicorn": "18.0.1",
    "gherkin-lint": "3.3.6",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rm": "^0.1.8",
    "semantic-release": "15.14.0",
    "sinon": "7.5.0",
    "sinon-chai": "^3.5.0",
    "tiny-types": "1.12.1",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslib": "^1.10.0",
    "typescript": "3.2.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 150
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick --staged"
    ],
    "*.ts": [
      "npm run lint:fix"
    ],
    "*.feature": [
      "npm run lint:gherkin"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
